# Copy this file to `.env` and fill in values before running the server locally or when configuring environment variables on your host.
# PORT is optional; host providers set this automatically. If running locally, default is 3000.
PORT=3000

# Twilio credentials (optional). If set, the server will try to send SMS confirmations.
# You must create a Twilio account and use the account SID, auth token, and a Twilio phone number in E.164 format (e.g. +1234567890).
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM=+1xxxxxxxxxx

# Optional: override site base URL used for links in emails/messages (not used by default)
SITE_BASE_URL=http://localhost:3000
